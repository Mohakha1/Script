-----------------------------------
-- محتويات تبويب "اللاعب"
-----------------------------------
do
    local cf = contentFrames["اللاعب"]
    local y = 10
    local tb1 = createTextbox(cf, "قوة القفز", tostring(jumpPower), function(val)
        jumpPower = tonumber(val) or 150
        updateJumpPower()
    end)
    tb1.Position = UDim2.new(0,10,0,y)
    y = y + 40
    
    local tb2 = createTextbox(cf, "سرعة المشي", tostring(walkSpeed), function(val)
        walkSpeed = tonumber(val) or 50
        updateWalkSpeed()
    end)
    tb2.Position = UDim2.new(0,10,0,y)
    y = y + 40
    
    local tgl1 = createToggle(cf, "تفعيل القفز", function(state)
        enableJump = state
        updateJumpPower()
    end)
    tgl1.Position = UDim2.new(0,10,0,y)
    y = y + 50
    
    local tgl2 = createToggle(cf, "تفعيل السرعة", function(state)
        enableSpeed = state
        updateWalkSpeed()
    end)
    tgl2.Position = UDim2.new(0,10,0,y)
    y = y + 50
    
    createButton(cf, "تنقل", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Mohakha1/-/refs/heads/main/Teleport.txt"))()
    end).Position = UDim2.new(0,10,0,y)
    y = y + 50
    
    createToggle(cf, "Noclip", function(state)
        if state then
            noclipConn = RunService.Stepped:Connect(function()
                if localPlayer.Character then
                    for _, part in ipairs(localPlayer.Character:GetDescendants()) do
                        if part:IsA("BasePart") then
                            part.CanCollide = false
                        end
                    end
                end
            end)
        else
            if noclipConn then noclipConn:Disconnect() end
        end
    end).Position = UDim2.new(0,10,0,y)
end

-----------------------------------
-- محتويات تبويب "أغراض تحكم"
-----------------------------------
do
    local cf = contentFrames["أغراض تحكم"]
    local y = 10
    createToggle(cf, "إزالة لاق", function(state)
        if state then
            for _, v in ipairs(workspace:GetDescendants()) do
                if v:IsA("ParticleEmitter") or v:IsA("Trail") or v:IsA("Smoke") or v:IsA("Fire") or v:IsA("Sparkles") then
                    v:Destroy()
                end
            end
            Notify("تنبيه", "تم إزالة اللّاق")
        end
    end).Position = UDim2.new(0,10,0,y)
    y = y + 50
    
    createToggle(cf, "تسلق جدران", function(state)
        if state then
            wallClimbConn = RunService.Heartbeat:Connect(function()
                local hrp = localPlayer.Character and localPlayer.Character:FindFirstChild("HumanoidRootPart")
                if hrp then
                    local ray = Ray.new(hrp.Position, hrp.CFrame.LookVector * 3)
                    if workspace:FindPartOnRay(ray, localPlayer.Character) then
                        hrp.Velocity = Vector3.new(0,50,0)
                    end
                end
            end)
        else
            if wallClimbConn then wallClimbConn:Disconnect() end
        end
    end).Position = UDim2.new(0,10,0,y)
    y = y + 50
    
    createButton(cf, "اختفاء", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/GGH52lan/GGH52lan/main/Invis.lua"))()
    end).Position = UDim2.new(0,10,0,y)
    y = y + 50
    
    createButton(cf, "طيران", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Mohakha1/-/refs/heads/main/Protected_7047204136562154%20(1).txt"))()
    end).Position = UDim2.new(0,10,0,y)
    y = y + 50
    
    createButton(cf, "كشف الاقفال", function()
        loadstring(game:HttpGet("https://raw.githubusercontent.com/Mohakha1/-/refs/heads/main/Tor.txt"))()
    end).Position = UDim2.new(0,10,0,y)
end

