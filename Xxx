    createButton(cf, "حقيبة النار", function()
        local args = { "PickingTools", "PaperbagFire" }
        local remote = ReplicatedStorage.RE and ReplicatedStorage.RE:FindFirstChild("1Too1l")
        if remote then remote:InvokeServer(unpack(args)) end
    end).Position = UDim2.new(0,10,0,y)
    y = y + 50
    createButton(cf, "كريستال 1", function()
        local args = { "PickingTools", "Crystals" }
