-----------------------------------
-- إعدادات اللاعب
-----------------------------------
local jumpPower, walkSpeed = 150, 50
local enableJump, enableSpeed = false, false

local function updateJumpPower()
    local char = localPlayer.Character
    if char then
        local hum = char:FindFirstChild("Humanoid")
        if hum then
            hum.JumpPower = enableJump and jumpPower or 50
        end
    end
end

local function updateWalkSpeed()
    local char = localPlayer.Character
    if char then
        local hum = char:FindFirstChild("Humanoid")
        if hum then
            hum.WalkSpeed = enableSpeed and walkSpeed or 16
        end
    end
end

-----------------------------------
-- إنشاء واجهة المنيو (GUI)
-----------------------------------
-- إنشاء ScreenGui
local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "CustomMenu"
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = localPlayer:WaitForChild("PlayerGui")

-- الإطار الرئيسي للمنيو
local mainFrame = Instance.new("Frame")
mainFrame.Name = "MainFrame"
mainFrame.Size = UDim2.new(0,600,0,400)
mainFrame.Position = UDim2.new(0.3,0,0.3,0)
mainFrame.BackgroundColor3 = Color3.fromRGB(40,40,40)
mainFrame.BackgroundTransparency = 0.2
mainFrame.Parent = ScreenGui

-- خاصية سحب الإطار
local UIS = game:GetService("UserInputService")
local dragging = false
local dragStart, startPos

mainFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 then
        dragging = true
        dragStart = input.Position
        startPos = mainFrame.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then dragging = false end
        end)
    end
end)
mainFrame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement and dragging then
        local delta = input.Position - dragStart
        mainFrame.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
    end
end)

-- عنوان المنيو
local titleLabel = Instance.new("TextLabel", mainFrame)
titleLabel.Size = UDim2.new(1,0,0,50)
titleLabel.BackgroundTransparency = 1
titleLabel.Text = "منيو محمد TN"
titleLabel.Font = Enum.Font.GothamBold
titleLabel.TextScaled = true
titleLabel.TextColor3 = Color3.new(1,1,1)

-- إطار أزرار التبويبات
local tabsFrame = Instance.new("Frame", mainFrame)
tabsFrame.Size = UDim2.new(1,0,0,40)
tabsFrame.Position = UDim2.new(0,0,0,50)
tabsFrame.BackgroundTransparency = 1

-- إطار المحتوى
local contentFrame = Instance.new("Frame", mainFrame)
contentFrame.Size = UDim2.new(1,0,1,-90)
contentFrame.Position = UDim2.new(0,0,0,90)
contentFrame.BackgroundTransparency = 1

-----------------------------------
-- إنشاء تبويبات ومحتوياتها
-----------------------------------
local currentTab = nil
local tabButtons = {}
local contentFrames = {}

local function createTabButton(name, pos)
    local btn = Instance.new("TextButton", tabsFrame)
    btn.Size = UDim2.new(0,100,1,0)
    btn.Position = UDim2.new(0, pos, 0, 0)
    btn.BackgroundColor3 = Color3.fromRGB(70,70,70)
    btn.Font = Enum.Font.Gotham
    btn.TextScaled = true
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Text = name
    return btn
end

local function createContentFrame(name)
    local frame = Instance.new("Frame", contentFrame)
    frame.Name = name
    frame.Size = UDim2.new(1,0,1,0)
    frame.BackgroundTransparency = 1
    frame.Visible = false
    return frame
end

local tabNames = {"الرئيسية", "اللاعب", "أغراض تحكم", "سكربتات", "استهداف", "أغراض", "ألوان"}
for i, name in ipairs(tabNames) do
    tabButtons[name] = createTabButton(name, (i-1)*100)
    contentFrames[name] = createContentFrame(name)
    tabButtons[name].MouseButton1Click:Connect(function()
        if currentTab then currentTab.Visible = false end
        currentTab = contentFrames[name]
        currentTab.Visible = true
        for tname, tbtn in pairs(tabButtons) do
            tbtn.BackgroundColor3 = (tname == name) and Color3.fromRGB(90,90,90) or Color3.fromRGB(70,70,70)
        end
    end)
end
-- تحديد التبويب الافتراضي
currentTab = contentFrames["الرئيسية"]
currentTab.Visible = true
tabButtons["الرئيسية"].BackgroundColor3 = Color3.fromRGB(90,90,90)

-- دوال مساعدة لإنشاء عناصر الواجهة
local function createButton(parent, text, callback)
    local btn = Instance.new("TextButton", parent)
    btn.Size = UDim2.new(0,150,0,40)
    btn.BackgroundColor3 = Color3.fromRGB(80,80,80)
    btn.Font = Enum.Font.GothamBold
    btn.TextScaled = true
    btn.TextColor3 = Color3.new(1,1,1)
    btn.Text = text
    btn.MouseButton1Click:Connect(callback)
    return btn
end

local function createToggle(parent, text, callback)
    local toggle = Instance.new("TextButton", parent)
    toggle.Size = UDim2.new(0,150,0,40)
    toggle.BackgroundColor3 = Color3.fromRGB(80,80,80)
    toggle.Font = Enum.Font.GothamBold
    toggle.TextScaled = true
    toggle.TextColor3 = Color3.new(1,1,1)
    local state = false
    toggle.Text = text .. " : OFF"
    toggle.MouseButton1Click:Connect(function()
        state = not state
        toggle.Text = text .. " : " .. (state and "ON" or "OFF")
        callback(state)
    end)
    return toggle
end

local function createTextbox(parent, text, default, callback)
    local container = Instance.new("Frame", parent)
    container.Size = UDim2.new(0,320,0,30)
    container.BackgroundTransparency = 1
    
    local label = Instance.new("TextLabel", container)
    label.Size = UDim2.new(0,150,1,0)
    label.BackgroundTransparency = 1
    label.Font = Enum.Font.Gotham
    label.TextScaled = true
    label.TextColor3 = Color3.new(1,1,1)
    label.Text = text
    
    local box = Instance.new("TextBox", container)
    box.Size = UDim2.new(0,150,1,0)
    box.Position = UDim2.new(0,160,0,0)
    box.BackgroundColor3 = Color3.fromRGB(100,100,100)
    box.Font = Enum.Font.Gotham
    box.TextScaled = true
    box.TextColor3 = Color3.new(1,1,1)
    box.Text = default or ""
    box.FocusLost:Connect(function(enterPressed)
        callback(box.Text)
    end)
    
    return container
end

